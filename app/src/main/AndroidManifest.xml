<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.example.liftlog">

<uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.CAMERA" />
<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

<application
    android:name=".MyApplication"
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.LiftLog">
    <activity
        android:name=".Login"
        android:label="@string/app_name">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name=".workout_view"
        android:label="Workout"/>
    <activity android:name=".MainActivity"
        android:label="@string/app_name"/>
    <activity android:name=".Register"
        android:label="@string/app_name"/>
    <activity
        android:name=".Profile"
        android:windowSoftInputMode="adjustPan"
        android:label="Profile"/>
    <activity android:name=".Routines"
        android:label="Routines" />
    <activity android:name=".MyWorkouts"
        android:label="My Workouts" />
    <activity android:name=".RoutineView"
        android:label="Routine" />
    <activity android:name=".num_reps_popup" android:theme="@style/DialogTheme.popup"/>
    <activity android:name=".new_max_weight_popup" android:theme="@style/DialogTheme.popup"/>
</application>

</manifest>